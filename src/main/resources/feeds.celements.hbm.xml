<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class entity-name="XWiki.AggregatorURLClass" table="feeds_aggregatorurl">
    <id name="id" type="integer" unsaved-value="any">
      <column name="agg_id" not-null="true" />
      <generator class="assigned" />
    </id>
    <property name="name" type="text">
      <column name="agg_name" />
    </property>
    <property name="url" type="text">
      <column name="agg_url" />
    </property>
    <list name="group" table="feeds_aggregatorurlgroups" lazy="false">
      <key>
        <column name="agl_id" />
      </key>
      <index column="agl_number" />
      <element column="agl_value" type="string" />
    </list>
    <property name="date" type="timestamp">
      <column name="agg_date" />
    </property>
    <property name="nb" type="integer">
      <column name="agg_nb" />
    </property>
  </class>
  <class entity-name="XWiki.AggregatorGroupClass" table="feeds_aggregatorgroup">
    <id name="id" type="integer" unsaved-value="any">
      <column name="agg_id" not-null="true" />
      <generator class="assigned" />
    </id>
    <property name="name" type="text">
      <column name="agg_name" />
    </property>
  </class>
  <class entity-name="XWiki.KeywordClass" table="feeds_keyword">
    <id name="id" type="integer" unsaved-value="any">
      <column name="key_id" not-null="true" />
      <generator class="assigned" />
    </id>
    <property name="name" type="text">
      <column name="key_name" />
    </property>
    <property name="group" type="text">
      <column name="key_group" />
    </property>
  </class>
  <class entity-name="XWiki.FeedEntryClass" table="feeds_feedentry">
    <id name="id" type="integer" unsaved-value="any">
      <column name="fee_id" not-null="true" />
      <generator class="assigned" />
    </id>
    <property name="title" type="text">
      <column name="fee_title" />
    </property>
    <property name="author" type="text">
      <column name="fee_author" />
    </property>
    <property name="feedurl" type="text">
      <column name="fee_feedurl" />
    </property>
    <property name="feedname" type="text">
      <column name="fee_feedname" />
    </property>
    <property name="url" type="text">
      <column name="fee_url" />
    </property>
    <property name="category" type="text">
      <column name="fee_category" />
    </property>
    <property name="content" type="text">
      <column name="fee_content" />
    </property>
    <property name="fullContent" type="text">
      <column name="fee_fullcontent" />
    </property>
    <property name="xml" type="text">
      <column name="fee_xml" />
    </property>
    <property name="date" type="timestamp">
      <column name="fee_date" />
    </property>
    <property name="flag" type="integer">
      <column name="fee_flag" default="0" />
    </property>
    <property name="read" type="integer">
      <column name="fee_read" default="0" />
    </property>
    <list name="tags" table="feeds_feedentrytags" lazy="false">
      <key>
        <column name="fet_id" />
      </key>
      <index column="fet_number" />
      <element column="fet_value" type="string" />
    </list>
  </class>
</hibernate-mapping>